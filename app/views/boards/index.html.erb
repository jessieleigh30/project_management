<div class="board_container">
<br/>
<%= link_to "Log Out", destroy_user_session_path, method: :delete, class: "btn grey" %> 
<%= link_to "Add New Board", new_board_path, class: "btn grey" %>

<div class="user">
  <h2><%= current_user.first_name %>'s Workspace </h2>
</div>
<ul>
  <% @boards.each do |board| %>
    <li>
      <%= link_to board.name, board %>
    </li>
  <% end %>
</ul>

</div>
<%= button_to "Log Out", destroy_user_session_path, method: :delete %>
<h2><%= current_user.first_name %>'s Boards</h2>

<hr />
<%= link_to "Add New Board", new_board_path %>
<h3>Your Boards</h3>
<ul>
  <% @boards.each do |board| %>
    <li>
      <%= link_to board.name, board %>
    </li>
  <% end %>
</ul>